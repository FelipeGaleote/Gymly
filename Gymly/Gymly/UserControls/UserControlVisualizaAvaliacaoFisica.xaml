<UserControl x:Class="Gymly.UserControls.UserControlVisualizaAvaliacaoFisica"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Gymly.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <DocumentViewer Name="docView"></DocumentViewer>
        <Button Name="voltar" VerticalAlignment="Top" HorizontalAlignment="Right" Foreground="Black"  BorderBrush="{x:Null}" Click="Voltar_Click" >
            <Button.Background>
                <ImageBrush ImageSource="../Image/voltar_button.png" Stretch="Uniform">
                    <ImageBrush.RelativeTransform>
                        <TransformGroup>
                            <ScaleTransform CenterY="1" CenterX="1" ScaleX="1" ScaleY="1"/>
                            <SkewTransform CenterY="0.5" CenterX="0.5"/>
                            <RotateTransform CenterY="0.5" CenterX="0.5"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </ImageBrush.RelativeTransform>
                </ImageBrush>
            </Button.Background>
        </Button>
    </Grid>
</UserControl>
